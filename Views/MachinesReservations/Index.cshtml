@model IEnumerable<MicrobiologyLab.Models.MachinesReservation>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.machine_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ResOrNot)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Experiment)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.machine_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResOrNot)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Experiment.Exp_Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.machResId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.machResId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.machResId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
